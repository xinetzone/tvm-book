     "text": [
      "if (False) {\n",
      "  free_var %a;\n",
      "  free_var %b;\n",
      "  add(%a, %b)\n",
      "} else {\n",
      "  multiply(%a, %b)\n",
      "}\n"
     ]
    }
   ],
   "source": [
    "from tvm import relay\n",
    "\n",
    "a, b = [relay.var(name) for name in \"ab\"]\n",
    "c = a + b\n",
    "d = a * b\n",
    "cond = relay.const(a == b)\n",
    "f = relay.If(cond, c, d)\n",
    "print(f)"
   ]
  }
